@model IEnumerable<BookCompanyManagement.Models.PrintShop>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PrintShopName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrintShopDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsDeleted)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PrintShopName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrintShopDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsDeleted)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PrintShopId }) |
            @Html.ActionLink("Details", "Details", new { id=item.PrintShopId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PrintShopId })
        </td>
    </tr>
}

</table>
